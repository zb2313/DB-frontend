\n  },\r\n  async mounted(){\r\n    let _this=this;\r\n    await  _this.get_ticket_info() ;\r\n    await  new Promise((resolve, reject) => {_this.get_checi_info() ;resolve()} );\r\n\r\n\r\n  }\r\n};\r\n</script>"]}]}